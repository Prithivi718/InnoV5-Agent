<xml xmlns="https://developers.google.com/blockly/xml">
    <block type="controls_if" id="1" x="100" y="100">
      <mutation else="1"></mutation>
  
      <value name="IF0">
        <block type="logic_compare" id="2">
          <field name="OP">EQ</field>
          <value name="A">
            <block type="variables_get" id="3">
              <field name="VAR">a</field>
            </block>
          </value>
          <value name="B">
            <block type="math_number" id="4">
              <field name="NUM">100</field>
            </block>
          </value>
        </block>
      </value>
  
      <statement name="DO0">
        <block type="text_print" id="5">
          <value name="TEXT">
            <block type="text" id="6">
              <field name="TEXT">a is 100</field>
            </block>
          </value>
        </block>
      </statement>
  
      <statement name="ELSE">
        <block type="text_print" id="7">
          <value name="TEXT">
            <block type="text" id="8">
              <field name="TEXT">a is not 100</field>
            </block>
          </value>
        </block>
      </statement>
    </block>
  </xml>
  