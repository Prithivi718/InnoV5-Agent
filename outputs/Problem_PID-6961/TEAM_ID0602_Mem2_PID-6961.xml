<xml xmlns="https://developers.google.com/blockly/xml">
    <block type="variables_set">
        <field name="VAR">command</field>
        <value name="VALUE">
            <block type="text_prompt_ext">
                <field name="TYPE">NUMBER</field>
                <field name="TEXT"></field>
            </block>
        </value>
        <next>
            <block type="variables_set">
                <field name="VAR">min_limit</field>
                <value name="VALUE">
                    <block type="text_prompt_ext">
                        <field name="TYPE">NUMBER</field>
                        <field name="TEXT"></field>
                    </block>
                </value>
                <next>
                    <block type="variables_set">
                        <field name="VAR">max_limit</field>
                        <value name="VALUE">
                            <block type="text_prompt_ext">
                                <field name="TYPE">NUMBER</field>
                                <field name="TEXT"></field>
                            </block>
                        </value>
                        <next>
                            <block type="controls_if">
                                <value name="IF0">
                                    <block type="logic_operation">
                                        <field name="OP">AND</field>
                                        <value name="A">
                                            <block type="logic_compare">
                                                <field name="OP">GTE</field>
                                                <value name="A">
                                                    <block type="variables_get">
                                                        <field name="VAR">command</field>
                                                    </block>
                                                </value>
                                                <value name="B">
                                                    <block type="variables_get">
                                                        <field name="VAR">min_limit</field>
                                                    </block>
                                                </value>
                                            </block>
                                        </value>
                                        <value name="B">
                                            <block type="logic_compare">
                                                <field name="OP">LTE</field>
                                                <value name="A">
                                                    <block type="variables_get">
                                                        <field name="VAR">command</field>
                                                    </block>
                                                </value>
                                                <value name="B">
                                                    <block type="variables_get">
                                                        <field name="VAR">max_limit</field>
                                                    </block>
                                                </value>
                                            </block>
                                        </value>
                                    </block>
                                </value>
                                <statement name="DO0">
                                    <block type="text_print">
                                        <value name="TEXT">
                                            <block type="text">
                                                <field name="TEXT">valid</field>
                                            </block>
                                        </value>
                                    </block>
                                </statement>
                                <statement name="ELSE">
                                    <block type="text_print">
                                        <value name="TEXT">
                                            <block type="text">
                                                <field name="TEXT">invalid</field>
                                            </block>
                                        </value>
                                    </block>
                                </statement>
                            </block>
                        </next>
                    </block>
                </next>
            </block>
        </next>
    </block>
</xml>